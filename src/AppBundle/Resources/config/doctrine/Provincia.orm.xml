<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Provincia" table="provincia">
    <indexes>
      <index name="pais_id" columns="pais_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="descripcion" type="string" column="descripcion" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="abrev" type="string" column="abrev" length="10" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="activo" type="boolean" column="activo" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <many-to-one field="pais" target-entity="Pais" fetch="LAZY">
      <join-columns>
        <join-column name="pais_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
